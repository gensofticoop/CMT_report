	<queryString>
		<![CDATA[select
    b.account_no,
    b.account_name,
    nvl(m.balance_begin,0) as begin_amt
from finbankaccount b
left join (
    select account_no,
           min(balance_begin) keep (dense_rank first order by seq_no) as balance_begin
    from finbankstatement
    where EXTRACT(YEAR FROM operate_date) = $P{as_year}
    group by account_no
) m
on b.account_no = m.account_no
where b.coop_id = $P{as_coopid}

order by b.account_no]]>
	</queryString>
