	<subDataset name="Data" uuid="8d13cf61-186c-4feb-90c8-479ec396e52a">
		<parameter name="deptslip_No" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
	s.deptslip_no,
	s.deptslip_date,
	s.deptslip_amt,
	sd.seq_no,
	sd.slip_desc,
	sd.prncslip_amt,
	pre.prename_desc,
	(pre.prename_desc)|| TRIM(mt.deptaccount_name) || ' ' || TRIM(mt.deptaccount_sname) AS "MEMBER_NAME",
	mt.deptaccount_name,
	mt.deptaccount_sname,
	mt.deptaccount_no, --เลขสมาชิก
	mt.member_no, -- เลขสหกรณ์
	mt.address_no,
	mt.address_moo,
	mt.tambol_code,
	tamb.TAMBOL_DESC ,
	mt.district_code,
	mt.province_code,
	mt.postcode
FROM
	wcdeptslip s
JOIN wcdeptslipdet sd ON
	s.deptslip_no = sd.deptslip_no
	AND s.coop_id = sd.coop_id
	AND s.wc_id = sd.wc_id
JOIN wcreqdeposit mt ON
	mt.deptrequest_docno = s.deptaccount_no
	AND s.coop_id = mt.coop_id
	AND s.wc_id = mt.wc_id
LEFT JOIN mbucfprename pre ON
	mt.prename_code = pre.prename_code
LEFT JOIN MBUCFTAMBOL tamb ON
	mt.TAMBOL_CODE  = tamb.TAMBOL_CODE
WHERE
	s.deptslip_no = $P{deptslip_No}
ORDER BY
	sd.seq_no]]>
		</queryString>
