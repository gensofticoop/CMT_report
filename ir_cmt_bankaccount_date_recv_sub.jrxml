	<variable name="sum_recv_itemnet_amt" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{ITEMNET_AMT}==null?$F{ITEM_BANKSTM}:$F{ITEMNET_AMT}]]></variableExpression>
