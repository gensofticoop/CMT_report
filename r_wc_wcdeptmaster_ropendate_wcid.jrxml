	<queryString>
		<![CDATA[select
    wcdeptmaster.wc_id,
    wcdeptmaster.member_no,
    wcdeptmaster.deptaccount_no,
    trim(mbucfprename.prename_desc) as prename_desc,
    wcdeptmaster.deptopen_date,
    wcdeptmaster.wfbirthday_date,
    trim(wcdeptmaster.deptaccount_name) as deptaccount_name,
    trim(wcdeptmaster.deptaccount_sname) as deptaccount_sname,
    mbmembmaster.membgroup_code,
    mbucfmembgroup.membgroup_desc,
    wcdeptmaster.card_person,
    wcdeptmaster.deptclose_status,
    wcdeptmaster.prncbal,
    wccontcoop.coop_name,
    wcmembertype.wcmembertype_desc,
    wcdeptmaster.keeps_memno,
    wcdeptmaster.keep_deptno
from wcdeptmaster
    left join mbucfprename on wcdeptmaster.prename_code = mbucfprename.prename_code
    left join mbmembmaster on wcdeptmaster.member_no = mbmembmaster.member_no
    left join mbucfmembgroup on mbmembmaster.membgroup_code = mbucfmembgroup.membgroup_code
    left join wccontcoop on wcdeptmaster.coop_id = wccontcoop.coop_id
        and wcdeptmaster.wc_id = wccontcoop.wc_id
        and (wccontcoop.application = $P{as_app} OR $P{as_app} IS NULL)
    left join wcmembertype on wcdeptmaster.wftype_code = wcmembertype.wftype_code
        and wcdeptmaster.coop_id = wcmembertype.coop_id
        and wcdeptmaster.wc_id = wcmembertype.wc_id
where
    wcdeptmaster.coop_id = $P{as_coopid}
    and wcdeptmaster.wc_id = $P{as_wcid}
    and wcdeptmaster.deptclose_status = 0
    and trunc(wcdeptmaster.deptopen_date) between $P{start_date} and $P{end_date}
order by
    wcdeptmaster.deptaccount_no]]>
	</queryString>
